<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/swf/db" VERSION="20091210" COMMENT="XMLDB file for Moodle mod/swf"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
	<TABLES>
		<TABLE NAME="swf" COMMENT="Instances of SWF activity module" NEXT="swf_grade_items">
			<FIELDS>
				<FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id" NEXT="course"/>
				<FIELD NAME="course" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="course" PREVIOUS="id" NEXT="name"/>
				<FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="name" PREVIOUS="course" NEXT="intro"/>
				<FIELD NAME="intro" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="intro" PREVIOUS="name" NEXT="introformat"/>
				<FIELD NAME="introformat" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="introformat" PREVIOUS="intro" NEXT="timecreated"/>
				<FIELD NAME="timecreated" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="timecreated" PREVIOUS="introformat" NEXT="timemodified"/>
				<FIELD NAME="timemodified" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" COMMENT="timemodified" PREVIOUS="timecreated" NEXT="swfurl"/>
				<FIELD NAME="swfurl" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="swfurl" PREVIOUS="timemodified" NEXT="width"/>
				<FIELD NAME="width" TYPE="char" LENGTH="4" NOTNULL="false" UNSIGNED="true" DEFAULT="900" SEQUENCE="false" ENUM="false" COMMENT="width" PREVIOUS="swfurl" NEXT="height"/>
				<FIELD NAME="height" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="480" SEQUENCE="false" ENUM="false" COMMENT="height" PREVIOUS="width" NEXT="version"/>
				<FIELD NAME="version" TYPE="char" LENGTH="20" NOTNULL="true" DEFAULT="9.0.0" SEQUENCE="false" ENUM="false" COMMENT="version" PREVIOUS="height" NEXT="interaction"/>
				<FIELD NAME="interaction" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="interaction" PREVIOUS="version" NEXT="xmlurl"/>
				<FIELD NAME="xmlurl" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="xmlurl" PREVIOUS="interaction" NEXT="apikey"/>
				<FIELD NAME="apikey" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="apikey" PREVIOUS="xmlurl" NEXT="play"/>
				<FIELD NAME="play" TYPE="char" LENGTH="5" NOTNULL="true" DEFAULT="true" SEQUENCE="false" ENUM="false" COMMENT="play" PREVIOUS="apikey" NEXT="loopswf"/>
				<FIELD NAME="loopswf" TYPE="char" LENGTH="5" NOTNULL="true" DEFAULT="true" SEQUENCE="false" ENUM="false" COMMENT="loopswf" PREVIOUS="play" NEXT="menu"/>
				<FIELD NAME="menu" TYPE="char" LENGTH="5" NOTNULL="true" DEFAULT="true" SEQUENCE="false" ENUM="false" COMMENT="menu" PREVIOUS="loopswf" NEXT="quality"/>
				<FIELD NAME="quality" TYPE="char" LENGTH="20" NOTNULL="true" DEFAULT="best" SEQUENCE="false" ENUM="false" COMMENT="quality" PREVIOUS="menu" NEXT="scale"/>
				<FIELD NAME="scale" TYPE="char" LENGTH="30" NOTNULL="true" DEFAULT="noscale" SEQUENCE="false" ENUM="false" COMMENT="scale" PREVIOUS="quality" NEXT="salign"/>
				<FIELD NAME="salign" TYPE="char" LENGTH="2" NOTNULL="true" DEFAULT="tl" SEQUENCE="false" ENUM="false" COMMENT="salign" PREVIOUS="scale" NEXT="wmode"/>
				<FIELD NAME="wmode" TYPE="char" LENGTH="50" NOTNULL="true" DEFAULT="opaque" SEQUENCE="false" ENUM="false" COMMENT="wmode" PREVIOUS="salign" NEXT="bgcolor"/>
				<FIELD NAME="bgcolor" TYPE="char" LENGTH="6" NOTNULL="true" DEFAULT="FFFFFF" SEQUENCE="false" ENUM="false" COMMENT="bgcolour" PREVIOUS="wmode" NEXT="devicefont"/>
				<FIELD NAME="devicefont" TYPE="char" LENGTH="5" NOTNULL="true" DEFAULT="true" SEQUENCE="false" ENUM="false" COMMENT="devicefont" PREVIOUS="bgcolor" NEXT="seamlesstabbing"/>
				<FIELD NAME="seamlesstabbing" TYPE="char" LENGTH="5" NOTNULL="true" DEFAULT="true" SEQUENCE="false" ENUM="false" COMMENT="seamlesstabbing" PREVIOUS="devicefont" NEXT="allowfullscreen"/>
				<FIELD NAME="allowfullscreen" TYPE="char" LENGTH="5" NOTNULL="true" DEFAULT="false" SEQUENCE="false" ENUM="false" COMMENT="allowfullscreen" PREVIOUS="seamlesstabbing" NEXT="allowscriptaccess"/>
				<FIELD NAME="allowscriptaccess" TYPE="char" LENGTH="20" NOTNULL="true" DEFAULT="sameDomain" SEQUENCE="false" ENUM="false" COMMENT="allowscriptaccess" PREVIOUS="allowfullscreen" NEXT="allownetworking"/>
				<FIELD NAME="allownetworking" TYPE="char" LENGTH="20" NOTNULL="true" DEFAULT="all" SEQUENCE="false" ENUM="false" COMMENT="allownetworking" PREVIOUS="allowscriptaccess" NEXT="align"/>
				<FIELD NAME="align" TYPE="char" LENGTH="10" NOTNULL="true" DEFAULT="middle" SEQUENCE="false" ENUM="false" COMMENT="align" PREVIOUS="allownetworking" NEXT="name1"/>
				<FIELD NAME="name1" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="name1" PREVIOUS="align" NEXT="value1"/>
				<FIELD NAME="value1" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="value1" PREVIOUS="name1" NEXT="name2"/>
				<FIELD NAME="name2" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="name2" PREVIOUS="value1" NEXT="value2"/>
				<FIELD NAME="value2" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="value2" PREVIOUS="name2" NEXT="name3"/>
				<FIELD NAME="name3" TYPE="char" LENGTH="20" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="name3" PREVIOUS="value2" NEXT="value3"/>
				<FIELD NAME="value3" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="value3" PREVIOUS="name3" NEXT="grademax"/>
				<FIELD NAME="grademax" TYPE="int" LENGTH="5" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="grademax" PREVIOUS="value3" NEXT="gradepass"/>
				<FIELD NAME="gradepass" TYPE="int" LENGTH="5" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="gradepass" PREVIOUS="grademax" NEXT="feedback"/>
				<FIELD NAME="feedback" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="feedback" PREVIOUS="gradepass" NEXT="feedbacklink"/>
				<FIELD NAME="feedbacklink" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="feedbacklink" PREVIOUS="feedback" NEXT="skin"/>
				<FIELD NAME="skin" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="skin" PREVIOUS="feedbacklink" NEXT="configxml"/>
				<FIELD NAME="configxml" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="configxml" PREVIOUS="skin"/>
			</FIELDS>
		<KEYS>
			<KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="primary key of the table"/>
		</KEYS>
		<INDEXES>
			<INDEX NAME="course" UNIQUE="false" FIELDS="course"/>
		</INDEXES>
	</TABLE>
	<TABLE NAME="swf_grade_items" COMMENT="Instances of module in grade book" PREVIOUS="swf" NEXT="swf_grades">
		<FIELDS>
			<FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id of the table" NEXT="course"/>
			<FIELD NAME="course" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="course id" PREVIOUS="id" NEXT="instance"/>
			<FIELD NAME="instance" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="moodle module instance id" PREVIOUS="course" NEXT="swfid"/>
			<FIELD NAME="swfid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="swf module instance id" PREVIOUS="instance" NEXT="userid"/>
			<FIELD NAME="userid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="user id" PREVIOUS="swfid" NEXT="grade"/>
			<FIELD NAME="grade" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="user grade" PREVIOUS="userid" NEXT="maxgrade"/>
			<FIELD NAME="maxgrade" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="maximum possible grade" PREVIOUS="grade" NEXT="starttime"/>
			<FIELD NAME="starttime" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="time interaction started" PREVIOUS="maxgrade" NEXT="endtime"/>
			<FIELD NAME="endtime" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="time interaction ended" PREVIOUS="starttime" NEXT="timecreated"/>
			<FIELD NAME="timecreated" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="time created" PREVIOUS="endtime" NEXT="timemodified"/>
			<FIELD NAME="timemodified" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="time modified" PREVIOUS="timecreated" NEXT="feedback"/>
			<FIELD NAME="feedback" TYPE="text" LENGTH="medium" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="feedback comments" PREVIOUS="timemodified" NEXT="feedbacklink"/>
			<FIELD NAME="feedbacklink" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="link to resource" PREVIOUS="feedback" NEXT="answerdata"/>
			<FIELD NAME="answerdata" TYPE="text" LENGTH="medium" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="detailed answer data" PREVIOUS="feedbacklink"/>
		</FIELDS>
		<KEYS>
			<KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="primary key of the table"/>
		</KEYS>
	</TABLE>
	<TABLE NAME="swf_grades" COMMENT="User data pushed by learning interactions" PREVIOUS="swf_grade_items" NEXT="swf_interactions">
		<FIELDS>
			<FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id" NEXT="course"/>
			<FIELD NAME="course" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="course" PREVIOUS="id" NEXT="name"/>
			<FIELD NAME="name" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="name" PREVIOUS="course" NEXT="intro"/>
			<FIELD NAME="intro" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="intro" PREVIOUS="name" NEXT="swfid"/>
			<FIELD NAME="swfid" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="swfid" PREVIOUS="intro" NEXT="grademax"/>
			<FIELD NAME="grademax" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="grademax" PREVIOUS="swfid" NEXT="gradepass"/>
			<FIELD NAME="gradepass" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="gradepass" PREVIOUS="grademax" NEXT="feedback"/>
			<FIELD NAME="feedback" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="feedback" PREVIOUS="gradepass" NEXT="feedbacklink"/>
			<FIELD NAME="feedbacklink" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="feedbacklink" PREVIOUS="feedback" NEXT="timecreated"/>
			<FIELD NAME="timecreated" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="timecreated" PREVIOUS="feedbacklink" NEXT="timemodified"/>
			<FIELD NAME="timemodified" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="timemodified" PREVIOUS="timecreated"/>
		</FIELDS>
		<KEYS>
			<KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="primary key of the table"/>
		</KEYS>
	</TABLE>
	<TABLE NAME="swf_interactions" COMMENT="List of interaction data sets recorded in swf_interaction_data" PREVIOUS="swf_grades" NEXT="swf_interaction_data">
		<FIELDS>
			<FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id" NEXT="course"/>
			<FIELD NAME="course" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="course" PREVIOUS="id" NEXT="name"/>
			<FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="name" PREVIOUS="course" NEXT="intro"/>
			<FIELD NAME="intro" TYPE="text" LENGTH="medium" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="intro" PREVIOUS="name" NEXT="introformat"/>
			<FIELD NAME="introformat" TYPE="int" LENGTH="4" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="introformat" PREVIOUS="intro" NEXT="amftable"/>
			<FIELD NAME="amftable" TYPE="char" LENGTH="50" NOTNULL="false" DEFAULT="swf_interaction_data" SEQUENCE="false" ENUM="false" COMMENT="amftable" PREVIOUS="introformat" NEXT="timecreated"/>
			<FIELD NAME="timecreated" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="timecreated" PREVIOUS="amftable" NEXT="timemodified"/>
			<FIELD NAME="timemodified" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="timemodified" PREVIOUS="timecreated"/>
		</FIELDS>
		<KEYS>
			<KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="primary key of the table, please edit me"/>
		</KEYS>
	</TABLE>
		<TABLE NAME="swf_interaction_data" COMMENT="data for learning interaction data sets" PREVIOUS="swf_interactions">
			<FIELDS>
				<FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id of the table, please edit me" NEXT="interaction"/>
				<FIELD NAME="interaction" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="unique id in swf_interactions identifies learning interaction data set" PREVIOUS="id" NEXT="ordernum"/>
				<FIELD NAME="ordernum" TYPE="int" LENGTH="4" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="order in which rows are presented in some learning applications" PREVIOUS="interaction" NEXT="pmp3"/>
				<FIELD NAME="pmp3" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="paragraph audio" PREVIOUS="ordernum" NEXT="qmp3"/>
				<FIELD NAME="qmp3" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="question audio" PREVIOUS="pmp3" NEXT="cmp3"/>
				<FIELD NAME="cmp3" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="correct answer audio" PREVIOUS="qmp3" NEXT="wmp3"/>
				<FIELD NAME="wmp3" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="wrong answer audio" PREVIOUS="cmp3" NEXT="smp3"/>
				<FIELD NAME="smp3" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="time stretched audio" PREVIOUS="wmp3" NEXT="image"/>
				<FIELD NAME="image" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="image URL" PREVIOUS="smp3" NEXT="video"/>
				<FIELD NAME="video" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="video URL" PREVIOUS="image" NEXT="ptext"/>
				<FIELD NAME="ptext" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="paragraph text" PREVIOUS="video" NEXT="qtext"/>
				<FIELD NAME="qtext" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="question text" PREVIOUS="ptext" NEXT="ctext"/>
				<FIELD NAME="ctext" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="correct answer text" PREVIOUS="qtext" NEXT="wtext"/>
				<FIELD NAME="wtext" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="wrong answer text" PREVIOUS="ctext" NEXT="keyword"/>
				<FIELD NAME="keyword" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="key word vocabulary" PREVIOUS="wtext" NEXT="timecreated"/>
				<FIELD NAME="timecreated" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="time created" PREVIOUS="keyword" NEXT="timemodified"/>
				<FIELD NAME="timemodified" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="time modified" PREVIOUS="timecreated"/>
			</FIELDS>
			<KEYS>
				<KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="primary key of the table, please edit me"/>
			</KEYS>
		</TABLE>
	</TABLES>
</XMLDB>